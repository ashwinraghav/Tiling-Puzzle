package puzzle;

public class Domino
{
	private Color color;
	private char val;
	private int id;
	
	public Domino(char val)
	{
		color = Color.Grey;
		this.val = val;
		setId(-1);
	}
	
	@Override
	public String toString()
	{
		return Character.toString(val);
	}
	
	public Color getColor()
	{
		return color;
	}
	
	public void setColor(Color newColor)
	{
		color = newColor;
	}

	public void setId(int id) {
		this.id = id;
	}

	public int getId() {
		return id;
	}
	
	@Override
	public boolean equals(Object obj)
	{
		
	}
}
